default menu.c32
prompt 0
timeout 30
ONTIMEOUT 2

menu title ########## PXE Boot Menu ##########

label 1
menu label ^1) Boot from local drive localboot
#LOCALBOOT 0
COM32 chain.c32
APPEND hd0

label 2
menu label ^2) Install CentOS 7 UUDEMPIVERSIo
kernel centos7_x64_everything_1804/images/pxeboot/vmlinuz
append initrd=centos7_x64_everything_1804/images/pxeboot/initrd.img inst.ks=http://172.28.0.253/centos7_x64_everything_1804/anaconda-ks.cfg
